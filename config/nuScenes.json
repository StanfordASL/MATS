{
  "batch_size": 256,
  "grad_clip": 1.0,
  "learning_rate_style": "exp",
  "learning_rate": 0.002,
  "min_learning_rate": 1e-05,
  "learning_decay_rate": 0.9999,
  "prediction_horizon": 6,
  "minimum_history_length": 1,
  "maximum_history_length": 8,
  "map_encoder": {
    "VEHICLE": {
      "heading_state_index": 6,
      "patch_size": [50, 10, 50, 90],
      "map_channels": 3,
      "hidden_channels": [10, 20, 10, 1],
      "output_size": 32,
      "masks": [5, 5, 5, 3],
      "strides": [2, 2, 1, 1],
      "dropout": 0.5
    }
  },
  "k": 1,
  "k_eval": 25,
  "kl_min": 0.07,
  "kl_weight": 100.0,
  "kl_weight_start": 0,
  "kl_decay_rate": 0.99995,
  "kl_crossover": 400,
  "kl_sigmoid_divisor": 4,
  "rnn_kwargs": {
    "dropout_keep_prob": 0.75
  },
  "MLP_dropout_keep_prob": 0.9,
  "enc_rnn_dim_edge": 32,
  "enc_rnn_dim_edge_influence": 32,
  "enc_rnn_dim_history": 32,
  "enc_rnn_dim_future": 32,
  "dec_rnn_dim": 128,
  "q_z_xy_MLP_dims": null,
  "p_z_x_MLP_dims": 32,
  "GMM_components": 1,

  "log_p_yt_xz_max": 6,
  "log_p_yt_xz_min": null,

  "N": 1,
  "K": 5,
  "tau_init": 2.0,
  "tau_final": 0.05,
  "tau_decay_rate": 0.997,
  "use_z_logit_clipping": true,
  "z_logit_clip_start": 0.05,
  "z_logit_clip_final": 5.0,
  "z_logit_clip_crossover": 300,
  "z_logit_clip_divisor": 5,
  "dynamic": {
    "PEDESTRIAN": {
      "name": "DoubleIntegrator",
      "distribution": true,
      "limits": {}
    },
    "VEHICLE": {
      "name": "Unicycle",
      "distribution": true,
      "limits": {
        "max_a": 4,
        "min_a": -5,
        "max_heading_change": 0.7,
        "min_heading_change": -0.7
      }
    }
  },
  "state": {
    "PEDESTRIAN": {
      "position": ["x", "y"],
      "velocity": ["x", "y"],
      "acceleration": ["x", "y"]
    },
    "VEHICLE": {
      "position": ["x", "y"],
      "heading": ["\u00b0", "d\u00b0"],
      "velocity": ["norm"],
      "acceleration": ["norm"]
    }
  },

  "pred_state": {
    "PEDESTRIAN": {
      "position": ["x", "y"],
      "velocity": ["x", "y"]
    },
    "VEHICLE": {
      "position": ["x", "y"],
      "heading": ["\u00b0"],
      "velocity": ["norm"]
    }
  },

  "control_state": {
    "PEDESTRIAN": {
      "acceleration": ["x", "y"]
    },
    "VEHICLE": {
      "heading": ["d\u00b0"],
      "acceleration": ["norm"]
    }
  },

  "log_histograms": false,
  "init_scale": 1000.0
}
